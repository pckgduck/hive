! Perl extenstion off.
!URxvt.perl-ext:
!URxvt.perl-ext-common:


! Cntrl+Shift
URxvt.iso14755: false
URxvt.iso14755_52: false


urxvt.font: xft:inconsolata:pixelsize=16
!urxvt.font: xft:Terminus:pixelsize=12
URxvt.letterSpace: rxvt-unicode-cvs-patched-wideglyphs
URxvt.autoWrap: true

*foreground: #D7D0C7
*background: #151515

!black
! *color0:     #AF875F
! *color8:     #AF875F
*color0:     #101010
*color8:     #404040

!red
*color1:     #E84F4F
*color9:     #D23D3D

!green
*color2:     #B8D68C
*color10:    #A0CF5D

!yellow
*color3:     #E1AA5D
*color11:    #F39D21

!blue
*color4:     #7DC1CF
*color12:    #4E9FB1

!magenta
*color5:     #9B64FB
*color13:    #8542FF

!cyan
*color6:     #6D878D
*color14:    #42717B

!white
*color7:     #dddddd
*color15:    #dddddd

URxvt*borderColor:  #151515
URxvt*colorBD:      #ffffff
URxvt*cursorColor:  #ff8939
URxvt*cursorColor2: #151515


!You can change the colors of tabs with the following:

URxvt.tabbed.tabbar-fg: 2
URxvt.tabbed.tabbar-bg: 0
URxvt.tabbed.tab-fg: 3
URxvt.tabbed.tab-bg: 0

URxvt.perl-ext-common: default,tabbed,matcher
URxvt.tabbed.new-button: true
URXvt.tabbed.reopen-on-close: yes
URxvt.keysym.Control-Shift-t: perl:tabbedex:new_tab
URxvt.keysym.Shifft-Right: perl:tabbedex:next_tab
URxvt.keysym.Shift-Left: perl:tabbedex:prev_tab
!URxvt.tabbed.no-tabbedex-keys: true
